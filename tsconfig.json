  "compilerOptions": {
    "module": "NodeNext",
    "target": "ES2017",
    "sourceMap": true,
    "rootDir": "./src",
    "outDir": "./out",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "moduleResolution": "NodeNext",
    "skipLibCheck": true
  }
}
